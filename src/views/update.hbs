            <tr>
              
              <td ><input type="text"  name="Vin_Suv" placeholder={{Vin}}></td>
              <td ><input type="text" id="inputdesign" name="Fuel_efficiency_suv" placeholder={{Fuel_efficiency}}> cc</td>
              <td ><input type="text" id="brand" name="Brand_suv" placeholder={{Brand}}></td>
              <td ><input type="text" id="inputdesign" name="Price_suv" placeholder={{Price}}> $</td>
              <td ><input type="text" id="inputdesign" name="Sid_suv" placeholder={{Sid}}></td>
              <td ><input type="text" id="inputdesign" name="Size_suv" placeholder={{Size}}></td>
              <td ><input type="text" id="inputdesign" name="Num_seats_suv" placeholder={{Num_seats}}></td>
              <td>
                      <button
                        style="margin-left: 10px;"
                        name="suvupdateBtn"
                        type="submit"
                        class="suvupdateBtn"
                        value="{{Vin}}"
                        formaction="/update"
                      >
                        수정
                      </button></td>
          </tr>
          <td>
                      <button
                        style="margin-left: 10px;"
                        name="deleteSuvBtn"
                        type="submit"
                        class="deleteSuvBtn"
                        value="{{Vin}}"
                        formaction="/update"
                      >
                        삭제
                      </button></td>
             </tr>
         </from>
         {{/updateSuv}}
        </table>
      </div>
      </div>
      <div class="main-top">
      <span class="main-top_title">Truck 종류 </span>
      <div class="main-top_table">
        <table>
          <tr style="background-color: #5f5fc4;">
